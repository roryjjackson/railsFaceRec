<p style="color: green"><%= notice %></p>

<h1>Photos</h1>

<div id="photos">
  <% @photos.each do |photo| %>
    <%= render photo %>
    <p>
      <%= link_to "Show this photo", api_v1_photo_path(photo) %>
    </p>
  <% end %>
</div>

<%= link_to "New photo", new_api_v1_photo_path if policy(Photo).new? %>
